//Delivery Queue Senior Project

#include "mainwindow.h"
#include "ui_mainwindow.h"
end first file
MainWindow::MainWindow(QWidget *parent) :
    QMainWindow(parent),
    ui(new Ui::MainWindow)
{
    ui->setupUi(this);
//Classification ComboBox
    ui->Classification->addItem("Top Secret");
    ui->Classification->addItem("Secret");
    ui->Classification->addItem("Confidential");
    ui->Classification->addItem("Unclassified");
//Location ComboBox

    ui->Location ->addItem("Area 51,Nevada");
    ui->Location ->addItem("King's Bay,Georgia");
    ui->Location ->addItem("Pensacola,Florida");
    ui->Location ->addItem("Great Lakes,Illinois");
    ui->Location ->addItem("Bremerton,Washington");
    ui->Location ->addItem("New London,Connecticut");
    ui->Location ->addItem("Rota, Spain");
//mediaType Combobox

    ui->mediaType->addItem("Software");
    ui->mediaType->addItem("Hardware");
    ui->mediaType->addItem("Documentation");

//staffinglevel Combobox

    ui->staffingLevel->addItem("1");
    ui->staffingLevel->addItem("2");
    ui->staffingLevel->addItem("3");

//deliveryMethod Combobox
    ui->deliveryMethod->addItem("Hand Carried");
    ui->deliveryMethod->addItem("Shipping");
}

MainWindow::~MainWindow()
{
    delete ui;
}
